{
    "name": "terrain",
    "version": "1.0.0",
    "author": "✨ Alex ✨ <alex@dytry.ch>",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf dist",
        "clean:cache": "yarn run clean && rm -rf .parcel-cache",
        "start": "yarn run serve",
        "serve": "yarn run clean && parcel serve src/index.html",
        "build": "yarn run clean && NODE_ENV=production parcel build src/index.html --public-url https://alex.dytry.ch/toys/",
        "rust:release": "wasm-pack build crates/slomojs --release",
        "rust:dev": "wasm-pack build crates/slomojs --dev",
        "types": "tsc --noEmit",
        "test": "yarn run types",
        "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx,html,css,json}' ./package.json ./.prettierrc.json ./postcss.config.js ./README.md ./tailwind.config.js ./tsconfig.json"
    },
    "devDependencies": {
        "@parcel/transformer-glsl": "^2.6.2",
        "@parcel/transformer-toml": "^2.6.2",
        "@types/classnames": "^2.2.10",
        "@types/pixi.js": "^5.0.0",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/three": "^0.132.0",
        "@types/use-subscription": "^1.0.0",
        "glslify-bundle": "^5.1.1",
        "glslify-deps": "^1.3.1",
        "parcel": "^2.6.2",
        "process": "^0.11.10",
        "querystring-es3": "^0.2.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@parcel/plugin": "^2.6.2",
        "@types/color": "^3.0.3",
        "@types/d3-color": "^3.1.0",
        "@types/d3-interpolate": "^3.0.1",
        "@types/d3-quadtree": "^3.0.2",
        "@types/d3-scale-chromatic": "^3.0.0",
        "@types/delaunator": "^5.0.0",
        "@types/node": "^14.0.23",
        "autoprefixer": "^10.4.8",
        "classnames": "^2.2.6",
        "color": "^4.2.3",
        "d3-color": "^3.1.0",
        "d3-interpolate": "^3.0.1",
        "d3-quadtree": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-scale-chromatic": "^3.0.0",
        "delaunator": "^5.0.0",
        "open-simplex-noise": "^2.2.0",
        "pixi.js": "^6.5.1",
        "postcss": "^8.4.14",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rebound": "^0.1.0",
        "regenerator-runtime": "^0.13.7",
        "resize-observer-polyfill": "^1.5.1",
        "tailwindcss": "^3.1.7",
        "three": "^0.132.2",
        "use-resize-observer": "^9.0.2",
        "use-subscription": "^1.8.0",
        "utility-types": "^3.10.0",
        "webmidi": "^3.0.21"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "packageManager": "yarn@3.2.2"
}
